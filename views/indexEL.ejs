          <!-- Content Row -->
          <div class="card-body">
            <div class="table-responsive">
              <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">

          <thead>
            <tr>

              <th><center>Catalog Number</center></th>
              <th><center>Cantidad a reponer</center></th>
              <th><center>Fecha</center></th>


            </tr>

            <tbody id = "contenido">
              <% if(data){ %>
                 <% for (i = 0; i < data.length; i++ ) { %>
                   <tr>
                     <td><center><%= data[i].repo_catalog_number%> </center></td>
                     <td><center><%= data[i].cantidad%></center></td>
                     <td><center><%= new Date(data[i].fecha).getDay() +1 ;%>/<%= new Date(data[i].fecha).getMonth() +1 ;%>/<%= new Date(data[i].fecha).getFullYear() ;%> </center></td>
                   </tr>
                   <% } %>
              <% } %>

          </tbody>


          </thead>
        </tbody>
